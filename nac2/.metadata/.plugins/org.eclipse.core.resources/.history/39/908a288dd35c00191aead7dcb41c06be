package exercicios;

import java.util.Scanner;

public class Exercicio19 {
	public double salario;
	public double emprestimo;
	// professor eu não sei como funciona esse esquemad e emprestimo e parcela
	// minha lógica foi que só libera o empréstimo se o mesmo solicitado for menor ou igual do que (salario *1.3)
	
	public void executar() {
		Scanner leia = new Scanner(System.in);
		System.out.println("Digite o valor do seu salario bruto");
		salario = leia.nextDouble();
		System.out.println("Solicite o valor do empréstimo: ");
		emprestimo = leia.nextDouble()
	}
	
	
}
